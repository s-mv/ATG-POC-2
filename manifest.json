{
  "manifest_version": 3,
  "name": "Linkedin Scraper",
  "description": "Base Level Extension",
  "version": "1.0",
  "permissions": [
    "tabs",
    "activeTab",
    "scripting",
    "offscreen"
  ],
  "host_permissions": [
    "https://*/*",
    "http://*/*",
    "http://localhost:5000/*"
  ],
  "action": {
    "default_popup": "hello.html",
    "default_icon": "icon.png"
  }
}